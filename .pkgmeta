package-as: MissionReportButtonPlus

enable-nolib-creation: no

ignore:
    - .github
    - .screenshots
    - .vscode
    - .gitignore
    - CHANGELOG.md
    - README.md
    # --> GitHub + Wago.io
    - README-CF.md
    # --> CurseForge
    - README-WI.bbcode
    # --> WoWInterface

manual-changelog:
    filename: CHANGELOG-LATEST.md
    markup-type: markdown

wowi-convert-changelog: yes
# Indicates the BigWigsMods packager to convert the changelog from Markdown to
# BBCode for WoWInterface.

license-output: LICENSE.txt
